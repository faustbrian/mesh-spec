{
    "name": "urn:cline:forrst:ext:atomic-lock:fn:release",
    "version": "1.0.0",
    "stability": "stable",
    "summary": "Release a lock with ownership verification",
    "arguments": [
        {
            "name": "key",
            "schema": {
                "type": "string",
                "minLength": 1,
                "maxLength": 200,
                "pattern": "^[a-zA-Z0-9\\-_:.]+$"
            },
            "required": true,
            "summary": null,
            "description": "Lock key (with scope prefix if applicable). Must contain only alphanumeric characters, dash, underscore, colon, and dot.",
            "default": null,
            "deprecated": null,
            "examples": null
        },
        {
            "name": "owner",
            "schema": {
                "type": "string",
                "minLength": 1,
                "format": "uuid"
            },
            "required": true,
            "summary": null,
            "description": "Owner token from lock acquisition (UUID format)",
            "default": null,
            "deprecated": null,
            "examples": null
        }
    ],
    "result": {
        "resource": null,
        "schema": {
            "type": "object",
            "properties": {
                "released": {
                    "type": "boolean",
                    "description": "Whether release was successful"
                },
                "key": {
                    "type": "string",
                    "description": "The lock key"
                }
            },
            "required": [
                "released",
                "key"
            ]
        },
        "collection": false,
        "description": "Lock release result"
    },
    "errors": [
        {
            "code": "PARSE_ERROR",
            "message": "Parse error"
        },
        {
            "code": "INVALID_REQUEST",
            "message": "Invalid request"
        },
        {
            "code": "FUNCTION_NOT_FOUND",
            "message": "Function not found"
        },
        {
            "code": "INVALID_ARGUMENTS",
            "message": "Invalid arguments"
        },
        {
            "code": "SCHEMA_VALIDATION_FAILED",
            "message": "Validation error"
        },
        {
            "code": "INTERNAL_ERROR",
            "message": "Internal error"
        },
        {
            "code": "INTERNAL_ERROR",
            "message": "Server error"
        },
        {
            "code": "UNAVAILABLE",
            "message": "Service unavailable"
        },
        {
            "code": "UNAUTHORIZED",
            "message": "Unauthorized"
        },
        {
            "code": "FORBIDDEN",
            "message": "Forbidden"
        },
        {
            "code": "RATE_LIMITED",
            "message": "Rate limited"
        },
        {
            "code": "LOCK_NOT_FOUND",
            "message": "Lock does not exist",
            "description": "The specified lock does not exist or has already expired",
            "details": null
